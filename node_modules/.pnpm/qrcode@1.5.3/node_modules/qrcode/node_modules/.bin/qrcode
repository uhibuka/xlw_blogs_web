#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/code/xlw-blogs/blogs_web/node_modules/.pnpm/qrcode@1.5.3/node_modules/qrcode/bin/node_modules:/mnt/e/code/xlw-blogs/blogs_web/node_modules/.pnpm/qrcode@1.5.3/node_modules/qrcode/node_modules:/mnt/e/code/xlw-blogs/blogs_web/node_modules/.pnpm/qrcode@1.5.3/node_modules:/mnt/e/code/xlw-blogs/blogs_web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/code/xlw-blogs/blogs_web/node_modules/.pnpm/qrcode@1.5.3/node_modules/qrcode/bin/node_modules:/mnt/e/code/xlw-blogs/blogs_web/node_modules/.pnpm/qrcode@1.5.3/node_modules/qrcode/node_modules:/mnt/e/code/xlw-blogs/blogs_web/node_modules/.pnpm/qrcode@1.5.3/node_modules:/mnt/e/code/xlw-blogs/blogs_web/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/qrcode" "$@"
else
  exec node  "$basedir/../../bin/qrcode" "$@"
fi
